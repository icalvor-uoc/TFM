stic colorant"], ["<https://calvoritmo.com/tfm/entity/99400425-498d-4760-9bb1-267325e1c6f1>", "<http://www.w3.org/2004/02/skos/core#narrower>", "<http://dbpedia.org/resource/Pharmaceutical_industry>"], ["<http://dbpedia.org/resource/Pharmaceutical_industry>", "<http://www.w3.org/2000/01/rdf-schema#label>", "pharmaceutical industry"], ["<https://calvoritmo.com/tfm/entity/99400425-498d-4760-9bb1-267325e1c6f1>", "<http://www.w3.org/2004/02/skos/core#narrower>", "<http://dbpedia.org/resource/Defoamer>"], ["<http://dbpedia.org/resource/Defoamer>", "<http://www.w3.org/2000/01/rdf-schema#label>", "defoamers"], ["<https://calvoritmo.com/tfm/entity/99400425-498d-4760-9bb1-267325e1c6f1>", "<http://www.w3.org/2004/02/skos/core#narrower>", "<http://dbpedia.org/resource/Anticaking_agent>"], ["<http://dbpedia.org/resource/Anticaking_agent>", "<http://www.w3.org/2000/01/rdf-schema#label>", "Anticaking agent"]]}]}